#include "radio_profile.h"
//------------------------------------------------------------------------------------------------------------------
static struct radio_profile_s radio_profiles[] = {
    { "default-ook", 433920, 165000, 0, 2 },
    { "433_42-ook",  433420, 165000, 0, 2 }   
};
int radio_profiles_cnt;
//------------------------------------------------------------------------------------------------------------------
void radio_profile_init(void)
{
  radio_profiles_cnt = sizeof(radio_profiles) / sizeof(radio_profile_t);
}
//------------------------------------------------------------------------------------------------------------------
int radio_profile_get_count(void)
{
    return radio_profiles_cnt;
}
//------------------------------------------------------------------------------------------------------------------
struct radio_profile_s* radio_profile_get(int idx)
{
    if (idx >=0 && idx < radio_profiles_cnt)
       return &radio_profiles[idx];
    else
       return 0;
}
